{
  "compilerOptions": {
    "outDir": "wwwroot",
    "target": "ES6",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "lib": [ "ES2015", "ES2015.Promise" ]
  },
  "exclude": [
    "node_modules"
  ]
}